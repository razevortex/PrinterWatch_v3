function auto_link()
{
    const user = {{ auto|safe }};
    let f = document.getElementById('auto')
    f.method = "post";
    let u = document.getElementById('auto-u')
    u.value = auto[0];
    let p = document.getElementById('auto-u')
    p.value = auto[1];
    }

function session(){
    let sess = {{ session|safe }}
    let bod = document.getElementById('bod')
    for(let i of sess){
        bod.innerHTML += i+"</br>"
}